<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper
    PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
    "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="codeQuery">

    <!-- select -->
    <select id="codeSelect" resultType="hashmap">
    SELECT
        CODE,
        CODE_DETAIL
    FROM
        BG_CODE
    WHERE
    	PARENT_CODE = 'BOT'
    ORDER BY IDX DESC
    </select>
    
</mapper>
